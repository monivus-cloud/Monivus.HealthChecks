{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Monivus": {
    "Exporter": {
      "ApplicationHealthCheckUrl": "https://localhost:7061/health",
      "MonivusCloudUrl": "https://localhost:7234/api",
      "ApiKey": "test"
    },
    "System": {
      "MemoryUsageThresholdPercent": 80
    },
    "Url": {
      "google": {
        "ExpectedStatusCodes": [ 200, 301, 302 ],
        "SlowResponseThreshold" : 2000
      }
    }
  }
}
